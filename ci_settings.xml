<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.1.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.1.0 http://maven.apache.org/xsd/settings-1.1.0.xsd">
	<!-- Gitlab CI/DI 配置 -->
	<servers>
		<!-- 配置 Gitlab 登录服务，用于部署打包上传。ID与pom.xml <repository> <id> 一致. -->
		<server>
			<id>gitlab-maven</id>
			<configuration>
				<httpHeaders>
					<property>
						<name>Job-Token</name>
						<value>${CI_JOB_TOKEN}</value>
					</property>
				</httpHeaders>
			</configuration>
		</server>

		<!-- 阿里云效(限制企业成员访问)登录用户名及密码，用于拉取拉取依赖和部署打包上传 -->
		<server>
			<id>rdc-releases</id>
			<username>63b629edd1660910422e0d73</username>
			<password>5e_zDEFRNI-9</password>
		</server>
		<server>
			<id>rdc-snapshots</id>
			<username>63b629edd1660910422e0d73</username>
			<password>5e_zDEFRNI-9</password>
		</server>
	</servers>

<!-- Gitlab mvn clean install -->
<profiles>
	<profile>
        <properties>
        <altReleaseDeploymentRepository>
            gitlab-maven::default::${CI_API_V4_URL}/projects/${CI_PROJECT_ID}/packages/maven/
        </altReleaseDeploymentRepository>
        <altSnapshotDeploymentRepository>
            gitlab-maven::default::${CI_API_V4_URL}/projects/${CI_PROJECT_ID}/packages/maven/
        </altSnapshotDeploymentRepository>
        </properties>
    </profile>
</profiles>


</settings>